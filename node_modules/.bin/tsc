#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/lix/Code/forks/vite-plugin-ssr/node_modules/.pnpm/typescript@4.8.4/node_modules/typescript/bin/node_modules:/Users/lix/Code/forks/vite-plugin-ssr/node_modules/.pnpm/typescript@4.8.4/node_modules/typescript/node_modules:/Users/lix/Code/forks/vite-plugin-ssr/node_modules/.pnpm/typescript@4.8.4/node_modules:/Users/lix/Code/forks/vite-plugin-ssr/node_modules/.pnpm/node_modules:/Users/lix/Code/forks/vite-plugin-ssr/node_modules:/Users/lix/Code/forks/node_modules:/Users/lix/Code/node_modules:/Users/lix/node_modules:/Users/node_modules:/node_modules:/Users/lix/Code/forks/vite-plugin-ssr/examples/react-full/node_modules:/Users/lix/Code/forks/vite-plugin-ssr/examples/node_modules"
else
  export NODE_PATH="$NODE_PATH:/Users/lix/Code/forks/vite-plugin-ssr/node_modules/.pnpm/typescript@4.8.4/node_modules/typescript/bin/node_modules:/Users/lix/Code/forks/vite-plugin-ssr/node_modules/.pnpm/typescript@4.8.4/node_modules/typescript/node_modules:/Users/lix/Code/forks/vite-plugin-ssr/node_modules/.pnpm/typescript@4.8.4/node_modules:/Users/lix/Code/forks/vite-plugin-ssr/node_modules/.pnpm/node_modules:/Users/lix/Code/forks/vite-plugin-ssr/node_modules:/Users/lix/Code/forks/node_modules:/Users/lix/Code/node_modules:/Users/lix/node_modules:/Users/node_modules:/node_modules:/Users/lix/Code/forks/vite-plugin-ssr/examples/react-full/node_modules:/Users/lix/Code/forks/vite-plugin-ssr/examples/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../node_modules/.pnpm/typescript@4.8.4/node_modules/typescript/bin/tsc" "$@"
else
  exec node  "$basedir/../../../../node_modules/.pnpm/typescript@4.8.4/node_modules/typescript/bin/tsc" "$@"
fi
