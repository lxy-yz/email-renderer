#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/lix/Code/forks/vite-plugin-ssr/vite-plugin-ssr/node/bin/node_modules:/Users/lix/Code/forks/vite-plugin-ssr/vite-plugin-ssr/node/node_modules:/Users/lix/Code/forks/vite-plugin-ssr/vite-plugin-ssr/node_modules:/Users/lix/Code/forks/vite-plugin-ssr/node_modules:/Users/lix/Code/forks/node_modules:/Users/lix/Code/node_modules:/Users/lix/node_modules:/Users/node_modules:/node_modules:/Users/lix/Code/forks/vite-plugin-ssr/examples/react-full/node_modules:/Users/lix/Code/forks/vite-plugin-ssr/examples/node_modules"
else
  export NODE_PATH="$NODE_PATH:/Users/lix/Code/forks/vite-plugin-ssr/vite-plugin-ssr/node/bin/node_modules:/Users/lix/Code/forks/vite-plugin-ssr/vite-plugin-ssr/node/node_modules:/Users/lix/Code/forks/vite-plugin-ssr/vite-plugin-ssr/node_modules:/Users/lix/Code/forks/vite-plugin-ssr/node_modules:/Users/lix/Code/forks/node_modules:/Users/lix/Code/node_modules:/Users/lix/node_modules:/Users/node_modules:/node_modules:/Users/lix/Code/forks/vite-plugin-ssr/examples/react-full/node_modules:/Users/lix/Code/forks/vite-plugin-ssr/examples/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../vite-plugin-ssr/node/bin/vite-plugin-ssr.js" "$@"
else
  exec node  "$basedir/../../../../vite-plugin-ssr/node/bin/vite-plugin-ssr.js" "$@"
fi
